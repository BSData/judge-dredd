        <profile id="1bbc-905e-795d-fdda" name="Block Champion" hidden="false" typeId="1ad0-6f7a-89c2-56c0" typeName="[4] Mount">
          <characteristics>
            <characteristic name="Move" typeId="f8e7-0c5a-630a-6072">6&quot;</characteristic>
